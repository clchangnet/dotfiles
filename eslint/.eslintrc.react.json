{
  "extends": [
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
     "ecmaFeatures": {
       "jsx": true,
       "modules": true
    },
     "ecmaVersion": 2018,
     "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  }
}
